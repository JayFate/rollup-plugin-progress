{
  "name": "xxteam-rollup-plugin-progress",
  "version": "0.0.1",
  "author": "xxman",
  "description": "Rollup Progress Plugin",
  "license": "MIT",
  "keywords": [
    "rollup",
    "plugin",
    "progress",
    "progressbar"
  ],
  "homepage": "https://gitee.com/godxxman/xxteam-rollup-plugin-progress",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/godxxman/xxteam-rollup-plugin-progress.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "commit": "git-cz",
    "build": "npx rollup --c",
    "prepublish": "pnpm run build",
    "lint": "npx eslint --fix --ext .ts,.js ./",
    "install-git-hook": "npx husky install && npx husky set .husky/pre-commit \"npx lint-staged\" && npx husky set .husky/commit-msg \"npx --no-install commitlint --edit $1\""
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.18.3",
    "rollup": "^2.75.6",
    "chalk": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "browserslist": "^4.20.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "rollup-plugin-cleandir": "^2.0.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{ts,js,json,md}": [
      "prettier --write"
    ],
    "**/*.{ts,js}": [
      "eslint --fix"
    ]
  }
}
